:belongsToSegment rdf:type owl:ObjectProperty ;
                  rdfs:domain :Customer ;
                  rdfs:range :CustomerSegment ;
                  rdfs:comment "Customer segment assigned." ;
                  rdfs:label "belongsToSegment"@en .


