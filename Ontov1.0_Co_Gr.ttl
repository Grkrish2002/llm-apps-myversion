  \"Analytics Relevance\": [
    \"Used to analyze payment method preferences\",
    \"identify trends in payment usage\",
    \"detect fraudulent transactions\",
    \"and optimize payment processing costs.\"
  ],
  \"Semantic Referential Classes\": [
    \"SellPhaseConcept\",
    \"SalesTransaction\",
    \"PaymentMethod\",
    \"Order\",
    \"Customer\"
